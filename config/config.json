{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "galeria-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/galeria/GaleriaWebPart.js",
          "manifest": "./src/webparts/galeria/GaleriaWebPart.manifest.json"
        }
      ]
    },
    "banner-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/banner/BannerWebPart.js",
          "manifest": "./src/webparts/banner/BannerWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "GaleriaWebPartStrings": "lib/webparts/galeria/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "BannerWebPartStrings": "lib/webparts/banner/loc/{locale}.js"
  }
}
